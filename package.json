{
    "name": "node-starter-typescript",
    "version": "1.0.0",
    "description": "A simple starter Node project with TypeScript included",
    "main": "app/index.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/szilarddoro/node-starter-typescript"
    },
    "bugs": {
        "url": "https://github.com/szilarddoro/node-starter-typescript/issues"
    },
    "scripts": {
        "start": "tsnd --respawn --transpile-only ./app",
        "start:prod": "nodemon ./build/index.js",
        "tsc": "tsc --build tsconfig.json",
        "build": "tsc && npm start",
        "test": "jest --watch"
    },
    "keywords": [
        "node"
    ],
    "author": "Szilard Doro <doroszilard@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.9",
        "fs": "0.0.1-security",
        "lodash.flatten": "^4.4.0",
        "morgan": "^1.9.1",
        "typescript": "^3.6.4",
        "winston": "^3.2.1",
        "dot-prop": ">=4.2.1",
        "minimist": ">=1.2.3",
        "kind-of": ">=6.0.3"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.0",
        "@types/express-fileupload": "^1.1.0",
        "@types/jest": "^24.0.19",
        "@types/lodash.flatten": "^4.4.6",
        "@types/morgan": "^1.7.37",
        "@types/winston": "^2.4.4",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4",
        "prettier": "^1.17.1",
        "ts-jest": "^24.1.0",
        "ts-node-dev": "^1.0.0-pre.43"
    }
}
